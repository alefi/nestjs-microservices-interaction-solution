{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig/base.json",
  "display": "NestJs",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "target": "ES2022",
    "lib": ["es2022"],
    "noEmit": false,
    "incremental": false,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": false,
    "skipLibCheck": true,
    "strictNullChecks": false
  }
}
